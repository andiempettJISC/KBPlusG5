<!doctype html>
<html lang="en" class="no-js">
<head>
    <meta name="layout" content="base"/>
    <parameter name="pagetitle" value="Finance" />
    <parameter name="pagestyle" value="Finance" />
    <parameter name="actionrow" value="subscriptionsdetail" />
</head>
<body class="finance">

<div class="row">
    <div class="col s12 l10">
        <h1 class="page-title">Finance Detail</h1>
    </div>
</div>


<!--***tab card section***-->
     <div class="row row-content">
        
        <div class="col s12 m6 l3">
           <div class="card-panel drop-line">
            	<p class="card-title">Order No</p>
            	<p class="card-caption"><span>12345</span></p>
           </div>
           <div class="card-panel drop-line">
            	<p class="card-title">Date of issue</p>
            	<p class="card-caption"><span>04.08.2017</span></p>
          </div>
        </div>

        <div class="col s12 m6 l3">

          <div class="card-panel">
            <p class="card-title">Start Date</p>
            <p class="card-caption"><span>04.08.2017</span></p>
         </div>

          <div class="card-panel hide-text">
            	<p class="card-title">End Date</p>
            	<p class="card-caption"><span>04.08.2017</span></p>
          </div>

        </div>

        <div class="col s12 m6 l3">
			<div class="card jisc_card small white">
				<div class="card-content text-navy">
					<span class="card-title">Amount</span>
					<div class="card-detail">
						<ul class="collection">
							<li class="collection-item plain"><span>Subtotal:</span> £1000</li>
							<li class="collection-item plain"><span>VAT:</span> £200</li>
							<li class="collection-item plain last">Total: £1200</li>
						</ul>
					</div>
				</div>
	        </div>
        </div>
        <div class="col s12 m6 l3">
        	<div class="card jisc_card small white">
				<div class="card-content text-navy">
					<span class="card-title">Local Currency (GBP)</span>
					<div class="card-detail">
						<ul class="collection">
							<li class="collection-item plain"><span>Subtotal:</span> £1000</li>
							<li class="collection-item plain"><span>VAT:</span> £200</li>
							<li class="collection-item plain last">Total: £1200</li>
						</ul>
					</div>
				</div>
	        </div>
        </div>

     </div>
    <!--***tab card section end***-->

	<div class="row">
	<!--*** Documents card***-->
		<div class="col s12 l6">
			<div class="card white mobilecard" style="min-height: 250px;">
			<!--*** Internal Cards ***-->
				<div class="internal-card-panel">
					<p class="card-title">Status</p>
					<p class="card-caption">Actual</p>
				</div>
			<div class="internal-card-panel">
				<p class="card-title">Element</p>
				<p class="card-caption">Content free</p>
			</div>

			<div class="internal-card-panel">
				<p class="card-title">Category</p>
				<p class="card-caption">Credit</p>
			</div>

			<div class="internal-card-panel">
				<p class="card-title">Element</p>
				<p class="card-caption">Other</p>
			</div>

			<!--*** Internal Cards end ***-->
			</div>

			<div class="row row-inset">
				<div class="col s12 m6">
					<div class="card-panel drop-line">
		            	<p class="card-title">Codes</p>
		            	<p class="card-caption">efe32471</p>
		           	</div>
	           	</div>

				<div class="col s12 m6">
		           	<div class="card-panel drop-line">
		            	<p class="card-title">Notes</p>
		            	<p class="card-caption">this is a note that allow you</p>
		           	</div>
	           	</div>
           	</div>


		</div>
	<!--*** Documents card end ***-->

		<div class="col s12 l6">
			<div class="card-panel drop-line">
            	<p class="card-title">Subscription</p>
            	<p class="card-caption">123Doc Limited/Jisc Collections/123Library e-book collection/2012-2015</p>
           	</div>

           	<div class="card-panel drop-line">
            	<p class="card-title">Package</p>
            	<p class="card-caption">123Doc Limited/Jisc Collections/123Library e-book collection/2012-2015</p>
           	</div>

           	<div class="card-panel drop-line">
            	<p class="card-title">Title</p>
            	<p class="card-caption">123Doc Limited/Jisc Collections/123Library e-book collection/2012-2015</p>
           	</div>
		</div>

	</div>

</body>
</html>
